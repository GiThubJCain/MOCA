   from dlytrn
  where to_char(dlytrn.trndte, 'YYYYMMDD') between to_char(sysdate - @dias_inicial, 'YYYYMMDD')
    and to_char(sysdate - @dias_final, 'YYYYMMDD')
    and dlytrn.actcod in ('OTHRPL', 'CASRPL', 'PALRPL')
    and dlytrn.oprcod in ('TRN', 'CRP', 'PRP')
    and dlytrn.to_arecod in ('CIALMTC', 'FAALMTC')
  group by to_char(dlytrn.trndte, 'MM/DD/YYYY HH24:MI:SS'),
        prt_client_id,
        usr_id,
        wh_id,
