    and to_char(ap_dlytrn.trndte, 'YYYYMMDD') between to_char(sysdate - @dias_inicial, 'YYYYMMDD')
    and to_char(sysdate - @dias_final, 'YYYYMMDD')
  group by to_char(ap_dlytrn.trndte, 'MM/DD/YYYY HH24:MI:SS'),
