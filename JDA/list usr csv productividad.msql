        sto_zone.sto_zone_id
 union
 select count(distinct dlytrn.usr_id) total,
        to_char(dlytrn.trndte, 'MM/DD/YYYY HH24:MI:SS') fecha,
        dlytrn.usr_id,
        dlytrn.prt_client_id,
